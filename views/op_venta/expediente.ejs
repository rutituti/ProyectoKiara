<%- include('../includes/head.ejs') %>
    <body>

      

        <div class="container-fluid">
        
            <%- include('../includes/sidebar.ejs', {
                ubicacion: ubicacion,
            }) %>            

           
            <section class="home-section ">
                <div class="w-100">


                        
                    <% if (numdocs.length>  0 || numdocs > 0) { %>
                    <div class=" row fixed-center container-fluid col-12 col-m-6 col-lg-10 bg-white border p-3 text-center"> 
                           
                          
                      <h5 class="fixed-center"> EXPEDIENTE:  <%=numdocs[0].tipoExpCliente%>  </h5>
                        <div class="row fixed-center">    
                            <% for (let r of numdocs) { %>
                              
                                <label class=" fixed-center input-group-btn">
                                    <span class="text-center   border p-1  container-fluid  col-12 col-m-6 col-lg-10 btn btn-light btn-file" id="doc">
                                    Agregue <%= r.Nombre%> dando click en el boton
                                    
                                    </span>
                                    <a href="/operacion/<%=ubicacion%>/expediente/<%= r.IDtipoExpCliente%>/<%= r.IDtipoDocCliente%>" class="btn btn-secondary btn-lg " style="font-size: 10px;" role="button" aria-disabled="true">Subir archivo</a>
                                </label>
                                
                              
                            <% } %>
                       
                    <% } else { %>
                        <h3>No hay casas registradas</h3>
                    <% } %>
           

            
                              
                           
                        
                              
                       

                        </div>
                    </div>
                </div>   
            </section>
        </div>
        

        <script src="/js/bootstrap.bundle.min.js"> </script>
        <script src="/js/sidenav.js"></script>
   
    </body>    
</html>