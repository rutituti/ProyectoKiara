<%- include('../includes/head.ejs', {
    isLoggedIn: isLoggedIn,
    user: user,
}) %>

<h1>Listado de usuarios</h1>
<div id="Usuarios">
    <% if (usuarios.length > 0) { %>
        <div class="row">
            <% for (let u of usuarios) { %>
                <div class="col s12 m4 l3">
                    <div class="card" style="width: 21rem;">
                        <img class="card-img-top" src="/images/casaprincipal.jpg" alt="Card image cap">
                        <div class="card-body">
                          <h4 class="card-title"><%= u.nombre %></h4>
                          <% console.log(u) %>
                          
                          
                          <p class="card-text"><%= u.Nombres + u.Primer_apellido%> </p>
                          <a href="fffff" class="btn btn-primary">BORRAR</a>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    <% } else { %>
        <h3>No hay clientes registrados.</h3>
    <% } %>
</div>

